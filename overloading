public class overloading{
public void display(){
System.out.println("Display method with no parameter");
}
public void display(int a){
System.out.println("Display method with one parameter:"+ a);
}
public void display(int a, int b){
System.out.println("Display method with two parameters:"+a+","+b);
}
public void display(String str){
System.out.println("Display method with String parameter:"+str);
}
public static void main(String[] args){
overloading obj = new overloading();
obj.display();
obj.display(10);
obj.display(10,20);
obj.display("hello World");
}
}

